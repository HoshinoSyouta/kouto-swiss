ks-grid-col-width = 60px
ks-grid-gut-width = 20px
ks-grid-columns = 12
ks-grid-computed-total-width = ( ks-grid-col-width + ks-grid-gut-width ) * ks-grid-columns
ks-grid-total-width = ks-grid-computed-total-width

ks-grid-config = {
    "column-width": ks-grid-col-width,
    "gutter-width": ks-grid-gut-width,
    "columns": ks-grid-columns,
    "computed-total-width": ks-grid-computed-total-width,
    "total-width": ks-grid-total-width
}

@import "grid"
@import "row"
@import "column"
@import "offset"
