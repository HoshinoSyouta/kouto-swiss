@import 'kouto-swiss'

button
    align-content stretch
    align-items stretch
    align-self auto

button
    animation none 0s ease 0s 1 normal none
    animation-delay 0
    animation-direction normal
    animation-duration 0
    animation-fill-mode none
    animation-iteration-count 1
    animation-name none
    animation-play-state running
    animation-timing-function ease

button
    appearance none

button
    backface-visibility visible

button
    background-clip border-box
    background-origin padding-box
    background-size auto auto

button
    clip-path none

button
    column-count auto
    column-fill balance
    column-gap normal
    column-rule medium none black
    column-rule-color black
    column-rule-style medium
    column-rule-width medium
    column-span none
    column-width auto
    columns auto auto

button
    flex 0 1 auto
    flex-basis auto
    flex-direction row
    flex-flow row nowrap
    flex-grow 0
    flex-shrink 1
    flex-wrap nowrap

button
    grid none
    grid-area none
    grid-auto-columns auto
    grid-auto-flow none
    grid-auto-position 1 / 1
    grid-auto-rows auto
    grid-column none
    grid-column-end auto
    grid-column-start auto
    grid-row none
    grid-row-end auto
    grid-row-start auto
    grid-template none
    grid-template-areas none
    grid-template-columns none
    grid-template-rows none

button
    justify-content auto

button
    mask border-box

button
    opacity .5
    opacity 1
    opacity inherit

button
    perspective none
    perspective-origin 50% 50%

button
    transform none
    transform-origin 50% 50% 0
    transform-style flat

button
    transition: 0s 0s all ease
    transition-delay: 0s
    transition-duration: 0s
    transition-property: all
    transition-timing-function: ease

ks-vendors-prefixes = o ms webkit moz

button
    mask border-box

ks-vendors-prefixes = null

button
    mask border-box
